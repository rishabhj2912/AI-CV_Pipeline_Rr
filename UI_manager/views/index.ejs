  let counter = -1;
  function printContent( oper_list ){
    if( counter > oper_list.length - 2 ){
      counter = counter + 0;
    }
    else{
      counter = counter + 1;
    }
    let n = counter + 1;
    return "Step " + n.toString( ) + " " + oper_list[counter];
  }
  function sleep(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
  }
  function sendPipeline(){
